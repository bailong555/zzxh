.myhome {
  width: 100%;
  height: 500rpx;
  position: relative;
}
.myhome image {
    width: 100%;
    height: 150px;
}
.myhome-div {
  width: 85%;
  height: 300rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
          justify-content: space-between;
  position: absolute;
  top: 30%;
}
.myhome-div1 {
  width: 50%;
  height: 100%;
}
.myhome-div1 image {
    width: 80%;
    height: 80%;
}
.myhome-div1 ._p {
    width: 70%;
    padding-left: 25rpx;
    margin: 0;
    pagging: 0;
    font-size: 29rpx;
}
.myhome-div1 ._p text {
      display: block;
      width: 30px;
      height: 15px;
      background: red;
      text-align: center;
      line-height: 15px;
      border-radius: 15rpx;
      color: #fff;
      font-size: 10px;
      float: right;
      margin-top: 3px;
}
.myhome-div2 {
  width: 50%;
  height: 100%;
  text-align: right;
  line-height: 550rpx;
}
.myhome-div2 image {
    width: 50%;
    height: 50%;
}
.myhome-main {
  padding: 0 22rpx;
}
.myhome-main-pueer {
  border-top: 1px solid #ccc;
  padding-top: 22rpx;
  margin-top: 22rpxn;
}
.myhome-main-div {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: url(data:image/png;base64,) no-repeat right;
  background-size: 8px;
}
.myhome-main-div text {
    font-size: 29rpx;
    float: left;
}
.myhome-main-div image {
    width: 20rpx;
    height: 20rpx;
    float: right;
    margin-top: 20rpx;
}
.myhome-pueer-div {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: url(data:image/png;base64,) no-repeat right;
  background-size: 8px;
}
.myhome-pueer-div text {
    font-size: 29rpx;
    float: left;
}
.myhome-pueer-div image {
    width: 20rpx;
    height: 20rpx;
    float: right;
    margin-top: 20rpx;
}

